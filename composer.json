{
    "name": "eoffice/eoffice",
    "type": "project",
    "license": "MIT",
    "authors": [
        {
            "name": "Anthonius Munthi",
            "email": "me@itstoni.com",
            "homepage": "https://itstoni.com"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true
    },
    "autoload": {
        "psr-4": {
            "EOffice\\": "src/",
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\EOffice\\": "tests/unit",
            "Functional\\EOffice\\": "tests/functional",
            "Integration\\EOffice\\": "tests/integration"
        }
    },
    "replace": {
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*"
    },
    "scripts": {
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd"
        },
        "prepare-test": [
            "@php ./bin/console cache:clear --env=test",
            "@php ./bin/console doctrine:database:create --env=test",
            "@php ./bin/console doctrine:schema:create --env=test",
            "@php ./bin/console lexik:jwt:generate-keypair --env=test"
        ],
        "post-install-cmd": [
            "@auto-scripts"
        ],
        "post-update-cmd": [
            "@auto-scripts"
        ]
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "5.3.*"
        },
        "branch-alias":{
            "dev-master": "1.0-dev"
        }
    },
    "require": {
        "php": ">=7.4",
        "ext-ctype": "*",
        "ext-iconv": "*",
        "ext-json": "*",
        "api-platform/core": "^2.6",
        "composer/package-versions-deprecated": "1.11.99.2",
        "doctrine/annotations": "^1.0",
        "doctrine/doctrine-bundle": "^2.4",
        "doctrine/doctrine-fixtures-bundle": "^3.4",
        "doctrine/doctrine-migrations-bundle": "^3.1",
        "doctrine/orm": "^2.9",
        "lexik/jwt-authentication-bundle": "^2.12",
        "nelmio/cors-bundle": "^2.1",
        "phpdocumentor/reflection-docblock": "^5.2",
        "symfony/asset": "5.3.*",
        "symfony/console": "5.3.*",
        "symfony/dotenv": "5.3.*",
        "symfony/expression-language": "5.3.*",
        "symfony/flex": "^1.3.1",
        "symfony/framework-bundle": "5.3.*",
        "symfony/property-access": "5.3.*",
        "symfony/property-info": "5.3.*",
        "symfony/proxy-manager-bridge": "5.3.*",
        "symfony/runtime": "5.3.*",
        "symfony/security-bundle": "5.3.*",
        "symfony/serializer": "5.3.*",
        "symfony/translation": "5.3.*",
        "symfony/twig-bundle": "5.3.*",
        "symfony/validator": "5.3.*",
        "symfony/yaml": "5.3.*"
    },
    "require-dev": {
        "behat/behat": "^3.8",
        "behatch/contexts": "^3.3",
        "friends-of-behat/mink": "^1.9",
        "friends-of-behat/mink-extension": "^2.5",
        "friends-of-behat/symfony-extension": "^2.1",
        "friendsofphp/php-cs-fixer": "^3.0",
        "liip/test-fixtures-bundle": "^1.12",
        "mockery/mockery": "^1.4",
        "phpstan/extension-installer": "^1.1",
        "phpstan/phpstan": "^0.12.94",
        "phpstan/phpstan-doctrine": "^0.12.42",
        "phpstan/phpstan-mockery": "^0.12.14",
        "phpstan/phpstan-phpunit": "^0.12.21",
        "phpunit/phpunit": "^9.5",
        "psalm/plugin-mockery": "^0.9.0",
        "psalm/plugin-phpunit": "^0.16.1",
        "psalm/plugin-symfony": "^2.4",
        "symfony/browser-kit": "5.3.*",
        "symfony/css-selector": "^5.3",
        "symfony/http-client": "5.3.*",
        "symfony/phpunit-bridge": "^5.3",
        "symfony/stopwatch": "^5.3",
        "symfony/web-profiler-bundle": "^5.3",
        "vimeo/psalm": "^4.9",
        "weirdan/doctrine-psalm-plugin": "^1.0"
    }
}
